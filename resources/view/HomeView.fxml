<?import javafx.scene.text.Text?>

<AnchorPane onKeyPressed="#keyPressed" prefHeight="800.0" prefWidth="600.0" stylesheets="@../css/Home.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scyjc1.frogger.controller.HomeController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="600.0">
         <image>
            <Image url="@../images/home_background.png" />
         </image>
      </ImageView>
      <VBox fx:id="optionBox" layoutX="25.0" layoutY="441.0" prefHeight="300.0" prefWidth="400.0" spacing="40.0">
         <children>
            <Text fill="#ffe500" stroke="#404040" strokeType="OUTSIDE" strokeWidth="2.0" text="&gt; START &lt;" textAlignment="CENTER" wrappingWidth="400.0" />
            <Text fill="#ffe500" stroke="#404040" strokeType="OUTSIDE" strokeWidth="2.0" text="LEADERBOARD" textAlignment="CENTER" wrappingWidth="400.0" />
            <Text fill="#ffe500" stroke="#404040" strokeType="OUTSIDE" strokeWidth="2.0" text="HELP" textAlignment="CENTER" wrappingWidth="400.0" />
            <Text fill="#ffe500" stroke="#404040" strokeType="OUTSIDE" strokeWidth="2.0" text="MUSIC: ON" textAlignment="CENTER" wrappingWidth="400.0" />
            <Text fill="#ffe500" stroke="#404040" strokeType="OUTSIDE" strokeWidth="2.0" text="QUIT" textAlignment="CENTER" wrappingWidth="400.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
